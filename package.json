    "chalk": "^4",
    "commitizen": "^4.2.4",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^8.1.0",
    "core-js": "^3.17.2",
    "cross-env": "^7.0.3",
