  "scripts": {
    "build": "webpack --config ./config/webpack.common.ts --env production",
    "build:beta": "cross-env VUE_APP_RELEASE_PROJECT_NAME=vue3-blog-admin VUE_APP_RELEASE_PROJECT_ENV=beta webpack --config ./config/webpack.common.ts --env production",
    "build:preview": "cross-env VUE_APP_RELEASE_PROJECT_NAME=vue3-blog-admin VUE_APP_RELEASE_PROJECT_ENV=preview webpack --config ./config/webpack.common.ts --env production",
    "build:prod": "cross-env VUE_APP_RELEASE_PROJECT_NAME=vue3-blog-admin VUE_APP_RELEASE_PROJECT_ENV=prod webpack --config ./config/webpack.common.ts --env production",
    "build:stats": "cross-env WEBPACK_ANALYZER_SWITCH=true webpack --config ./config/webpack.common.ts --env production",
    "commit": "cz",
    "lint": "eslint --config ./.eslintrc.js . --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:fix": "eslint --config ./.eslintrc.js . --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "push": "cz && standard-version",
    "release": "standard-version",
    "start": "webpack serve --config ./config/webpack.common.ts --env development",
    "start:beta": "cross-env VUE_APP_RELEASE_PROJECT_NAME=vue3-blog-admin VUE_APP_RELEASE_PROJECT_ENV=beta webpack serve --config ./config/webpack.common.ts --env development",
    "start:preview": "cross-env VUE_APP_RELEASE_PROJECT_NAME=vue3-blog-admin VUE_APP_RELEASE_PROJECT_ENV=preview webpack serve --config ./config/webpack.common.ts --env development",
    "start:prod": "cross-env VUE_APP_RELEASE_PROJECT_NAME=vue3-blog-admin VUE_APP_RELEASE_PROJECT_ENV=prod webpack serve --config ./config/webpack.common.ts --env production"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
